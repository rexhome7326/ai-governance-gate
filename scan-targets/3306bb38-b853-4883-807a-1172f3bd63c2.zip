  function sendHeight() {
    const el = document.getElementById('y-cny-widget-2026');
    if (el) {
      const height = el.offsetHeight;
      window.parent.postMessage({ type: 'resize', height: height }, window.location.origin);
    }
  }

  function init() {
    const scroller = document.getElementById('y-zodiac-scroller');
    const order = ['rat','ox','tiger','rabbit','dragon','snake','horse','sheep','monkey','rooster','dog','pig'];
    
    scroller.replaceChildren(); 

    order.forEach(k => {
      const item = document.createElement('div');
      item.className = 'y-zodiac-item' + (k === activeZodiac ? ' active' : '');
      const icon = document.createElement('span');
      icon.className = 'y-z-icon';
      icon.textContent = zData[k].icon;
      const text = document.createElement('span');
      text.className = 'y-z-text';
      text.textContent = zData[k].name;
      item.append(icon, text);
      item.onclick = () => {
        activeZodiac = k;
        document.querySelectorAll('.y-zodiac-item').forEach(i => i.classList.remove('active'));
        item.classList.add('active');
        render();
      };
      scroller.appendChild(item);
    });

    document.querySelectorAll('.y-seg-btn').forEach(btn => {
      btn.onclick = () => {
        activeMaster = btn.dataset.master;
        document.querySelectorAll('.y-seg-btn').forEach(b => b.classList.remove('active'));
        btn.classList.add('active');
        render();
      };
    });

    document.getElementById('y-scroll-btn-left').onclick = () => scroller.scrollBy({left:-150, behavior:'smooth'});
    document.getElementById('y-scroll-btn-right').onclick = () => scroller.scrollBy({left:150, behavior:'smooth'});
    
    render();
  }

